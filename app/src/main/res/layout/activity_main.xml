<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.liuxin.ilistviewdemo.ilistview.IListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:refreshenable="true"
        app:loadMoreenable="true"
        android:footerDividersEnabled="false"
        android:headerDividersEnabled="false"
        app:loadMoreFooterlayout="@layout/layout_ilistview_load_more_footer"
        app:refreshHeaderlayout="@layout/layout_ilistview_refresh_header"
        android:id="@+id/listView"
        >

    </com.liuxin.ilistviewdemo.ilistview.IListView>
</LinearLayout>
